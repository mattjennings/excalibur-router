{
  "name": "excalibur-router",
  "version": "0.0.5",
  "description": "A scene router for Excaliburjs",
  "type": "module",
  "main": "dist/excalibur-router.umd.cjs",
  "module": "dist/excalibur-router.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/excalibur-router.js",
      "require": "./dist/excalibur-router.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattjennings/excalibur-router.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "release": "npm run build && npm publish --access public"
  },
  "keywords": [],
  "author": "mattjennings",
  "license": "ISC",
  "peerDependencies": {
    "excalibur": "^0.27.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.38",
    "excalibur": "^0.27.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "prettier": "^2.6.2"
  }
}
